            ]
          }
        },
        "options": {
          "orientation": "horizontal",
          "colorMode": "background",
          "textMode": "value_and_name"
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "tenant",
          "type": "query",
          "query": "label_values(robot_fleet_healthy, tenant)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "options": [],
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          }
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Robot Fleet Events",
          "enable": true,
          "iconColor": "red",
          "datasource": "Prometheus",
          "expr": "changes(robot_fleet_down[5m]) > 0"
        }
      ]
    },
    "schemaVersion": 27,
    "version": 1,
    "links": [
      {
        "title": "Tenant Breakdown Dashboard",
        "url": "/d/tenant-breakdown",
        "type": "dashboards",
        "tags": ["tenant", "drill-down"]
      }
