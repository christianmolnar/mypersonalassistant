{
  "dashboard": {
    "id": null,
    "uid": "mission-control-dashboard",
    "title": "üöÄ Mission Control - Robot Fleet Health Overview",
    "description": "Enterprise Robot Fleet Mission Control with 1.65M Robots across Multi-Tenant Environment",
    "tags": ["mission-control", "robots", "fleet", "enterprise", "tenants"],
    "timezone": "browser",
    "refresh": "5s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "",
        "type": "text",
        "gridPos": {"h": 3, "w": 24, "x": 0, "y": 0},
        "options": {
          "mode": "html",
          "content": "<div style='text-align: center; font-size: 32px; font-weight: bold; color: #ffffff; background: linear-gradient(135deg, #1a1a2e 0%, #16213e 50%, #0f3460 100%); padding: 20px; border-radius: 10px; box-shadow: 0 4px 8px rgba(0,0,0,0.3);'><span style='color: #00d4aa;'>üöÄ MISSION CONTROL</span><br><span style='font-size: 18px; color: #b3b3b3;'>AI Robot Fleet Command Center - 1.65M Active Robots</span></div>"
        },
        "transparent": true
      },
      {
        "id": 2,
        "title": "TOTAL FLEET",
        "type": "stat",
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 3},
        "targets": [
          {
            "expr": "sum(robot_fleet_total)",
            "legendFormat": "Total Active Robots",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "fixed", "fixedColor": "white"},
            "unit": "short",
            "custom": {
              "displayMode": "basic",
              "textMode": "value_and_name"
            }
          }
        },
        "options": {
          "orientation": "horizontal",
          "colorMode": "background",
          "textMode": "value_and_name",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        }
      },
      {
        "id": 3,
        "title": "üü¢ HEALTHY ROBOTS",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 9},
        "targets": [
          {
            "expr": "sum(robot_fleet_healthy)",
            "legendFormat": "Healthy Robots",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "fixed", "fixedColor": "green"},
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "pointSize": 5,
              "fillOpacity": 30,
              "gradientMode": "opacity"
            }
          }
        },
        "options": {
          "tooltip": {"mode": "single"},
          "legend": {"displayMode": "hidden"}
        }
      },
      {
        "id": 4,
        "title": "üü° DEGRADED ROBOTS",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 9},
        "targets": [
          {
            "expr": "sum(robot_fleet_degraded)",
            "legendFormat": "Degraded Robots",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "fixed", "fixedColor": "yellow"},
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "pointSize": 5,
              "fillOpacity": 30,
              "gradientMode": "opacity"
            }
          }
        },
        "options": {
          "tooltip": {"mode": "single"},
          "legend": {"displayMode": "hidden"}
        }
      },
      {
        "id": 5,
        "title": "üü† CRITICAL ROBOTS",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 9},
        "targets": [
          {
            "expr": "sum(robot_fleet_critical)",
            "legendFormat": "Critical Robots",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "fixed", "fixedColor": "orange"},
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "pointSize": 5,
              "fillOpacity": 30,
              "gradientMode": "opacity"
            }
          }
        },
        "options": {
          "tooltip": {"mode": "single"},
          "legend": {"displayMode": "hidden"}
        }
      },
      {
        "id": 6,
        "title": "üî¥ DOWN ROBOTS",
