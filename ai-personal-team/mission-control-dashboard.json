            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "orange", "value": 1000},
                {"color": "yellow", "value": 2500},
                {"color": "green", "value": 4000}
              ]
            },
            "unit": "short",
            "links": [
              {
                "title": "View Tenant Breakdown",
                "url": "/d/tenant-breakdown?var-robot_type=${__field.labels.robot_type}&var-metric=throughput",
