      },
      {
        "id": 7,
        "title": "Robot Class Success Rates by Tenant",
        "type": "stat",
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 17},
        "targets": [
          {
            "expr": "robot_success_rate_percent{robot_type=\"image_generator\"}",
            "legendFormat": "üñºÔ∏è Image Generator",
            "refId": "A"
          },
          {
            "expr": "robot_success_rate_percent{robot_type=\"vinyl_researcher\"}",
            "legendFormat": "üìÄ Vinyl Researcher",
            "refId": "B"
          },
          {
            "expr": "robot_success_rate_percent{robot_type=\"communications\"}",
            "legendFormat": "‚úâÔ∏è Communications Agent",
            "refId": "C"
          },
          {
            "expr": "robot_success_rate_percent{robot_type=\"memorias_ai\"}",
