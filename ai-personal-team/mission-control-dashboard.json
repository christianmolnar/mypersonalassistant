          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "orange", "value": 50},
                {"color": "yellow", "value": 75},
                {"color": "green", "value": 90}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100,
            "links": [
              {
                "title": "Drill Down to Tenant View",
