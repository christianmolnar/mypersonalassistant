          },
          {
            "expr": "robot_throughput_transactions_per_minute{robot_type=\"fact_checker\"}",
            "legendFormat": "üïµÔ∏è Fact Checker",
            "refId": "E"
          },
          {
            "expr": "robot_throughput_transactions_per_minute{robot_type=\"finsight_ai\"}",
