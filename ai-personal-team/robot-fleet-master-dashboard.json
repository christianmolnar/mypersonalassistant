        "targets": [
          {
            "expr": "sum(robot_fleet_total)",
            "legendFormat": "Total Robots"
          },
          {
            "expr": "sum(robot_fleet_healthy)",
            "legendFormat": "Healthy"
          },
          {
            "expr": "sum(robot_fleet_degraded)",
            "legendFormat": "Degraded"
          },
          {
            "expr": "sum(robot_fleet_critical)",
            "legendFormat": "Critical"
          },
          {
            "expr": "sum(robot_fleet_down)",
            "legendFormat": "Down"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "orange", "value": 50},
                {"color": "yellow", "value": 75},
                {"color": "green", "value": 90}
              ]
            },
            "unit": "short",
            "custom": {
              "displayMode": "basic"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Total Robots"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "white"}}
              ]
