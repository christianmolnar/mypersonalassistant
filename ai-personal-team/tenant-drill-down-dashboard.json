          {
            "expr": "robot_fleet_healthy{tenant=\"$tenant\"}",
            "legendFormat": "{{robot_type}} - Healthy",
            "refId": "A"
          },
          {
            "expr": "robot_fleet_degraded{tenant=\"$tenant\"}",
            "legendFormat": "{{robot_type}} - Degraded",
            "refId": "B"
          },
          {
            "expr": "robot_fleet_critical{tenant=\"$tenant\"}",
            "legendFormat": "{{robot_type}} - Critical",
            "refId": "C"
          }
        ]
      },
      {
        "id": 4,
        "title": "üéØ $tenant - Robot Type Performance Matrix",
