        "targets": [
          {
            "expr": "sum(robot_fleet_total{tenant=\"$tenant\"})",
            "legendFormat": "Total Robots",
            "refId": "A"
          },
          {
            "expr": "sum(robot_fleet_healthy{tenant=\"$tenant\"})",
            "legendFormat": "Healthy",
            "refId": "B"
          },
          {
            "expr": "sum(robot_fleet_degraded{tenant=\"$tenant\"})",
            "legendFormat": "Degraded",
            "refId": "C"
          },
          {
            "expr": "sum(robot_fleet_critical{tenant=\"$tenant\"})",
            "legendFormat": "Critical",
            "refId": "D"
          },
          {
            "expr": "sum(robot_fleet_down{tenant=\"$tenant\"})",
            "legendFormat": "Down",
            "refId": "E"
          }
        ]
